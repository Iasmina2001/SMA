<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".intents.MainIntentActivity">

    <Button
        android:id="@+id/buttonViewGoogle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="create an action VIEW intent to view  Uri.parse(&quot;http://www.google.com&quot;) "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.108" />

    <Button
        android:id="@+id/buttonViewTel"
        android:layout_width="319dp"
        android:layout_height="74dp"
        android:text=" create an action VIEW intent to view  Uri.parse(&quot;tel:00401213456&quot;)"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.337" />

    <Button
        android:id="@+id/buttonMSALaunchGoogle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="create an action MSA.LAUNCH intent to view  Uri.parse(&quot;https://www.google.com&quot;) "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.542" />

    <Button
        android:id="@+id/buttonMSALaunchTel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="create an action MSA.LAUNCH intent to view  Uri.parse(&quot;tel:00401213456&quot;)"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.76" />
</androidx.constraintlayout.widget.ConstraintLayout>